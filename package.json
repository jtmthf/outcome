{
  "name": "@jtmthf/outcome",
  "version": "0.0.0",
  "description": "Rust-like Option and Result types for JavaScript",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./option": {
      "require": {
        "types": "./dist/option.d.ts",
        "default": "./option.js"
      },
      "import": {
        "types": "./dist/option.d.mts",
        "default": "./dist/option.mjs"
      }
    },
    "./result": {
      "require": {
        "types": "./dist/result.d.ts",
        "default": "./result.js"
      },
      "import": {
        "types": "./dist/result.d.mts",
        "default": "./dist/result.mjs"
      }
    },
    "./task": {
      "require": {
        "types": "./dist/task.d.ts",
        "default": "./task.js"
      },
      "import": {
        "types": "./dist/task.d.mts",
        "default": "./dist/task.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "rust",
    "option",
    "result",
    "match",
    "monad",
    "functional",
    "fp"
  ],
  "author": "Jack Moore",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "option",
    "result",
    "task"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@microsoft/api-extractor": "^7.47.9",
    "@vitest/coverage-v8": "^2.1.1",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b"
}
